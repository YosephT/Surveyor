<p id="notice"><%= notice %></p>

<h1>Listing User Responses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @surveys.each do |survey| %>
      <tr>
        <td><%= survey.title %></td>
        <td><%= survey.question %></td>
        <td><%= link_to 'Respond', user_response_path(survey) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User response', new_user_response_path %>
